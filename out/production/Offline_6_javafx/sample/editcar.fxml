<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="586.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.editCarController">
   <children>
      <Label layoutX="213.0" layoutY="148.0" prefHeight="30.0" prefWidth="181.0" text="Enter registration number :">
         <font>
            <Font name="Calisto MT" size="15.0" />
         </font></Label>
      <TextField fx:id="regText" layoutX="198.0" layoutY="192.0" prefHeight="30.0" prefWidth="212.0" />
      <Button fx:id="search" layoutX="215.0" layoutY="236.0" mnemonicParsing="false" onAction="#searchAction" text="Search">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="back" layoutX="339.0" layoutY="236.0" mnemonicParsing="false" onAction="#backAction" text="Back">
         <font>
            <Font size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
