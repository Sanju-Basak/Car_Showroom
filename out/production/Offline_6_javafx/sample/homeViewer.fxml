<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="586.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.HomeViewerController">
   <children>
      <VBox layoutX="373.0" layoutY="93.0" prefHeight="277.0" prefWidth="180.0">
         <children>
            <Button fx:id="viewcar" mnemonicParsing="false" onAction="#viewcarPressed" prefHeight="53.0" prefWidth="182.0" text="View all cars">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="regnum" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#regnumPressed" prefHeight="54.0" prefWidth="186.0" text="Search by registration number">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="makemodel" layoutX="10.0" layoutY="52.0" mnemonicParsing="false" onAction="#makemodelPressed" prefHeight="52.0" prefWidth="182.0" text="Search by make &amp; model">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="buycar" layoutX="10.0" layoutY="95.0" mnemonicParsing="false" onAction="#buycarPressed" prefHeight="48.0" prefWidth="182.0" text="Buy a car">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="logout" layoutX="10.0" layoutY="137.0" mnemonicParsing="false" onAction="#logoutAction" prefHeight="52.0" prefWidth="180.0" text="Logout">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
      </VBox>
      <Label layoutX="88.0" layoutY="93.0" text="Welcome Viewer !!">
         <font>
            <Font name="Calisto MT Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
