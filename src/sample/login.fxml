<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="584.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <Label layoutX="174.0" layoutY="184.0" text="Username :">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label layoutX="176.0" layoutY="245.0" text="Password :">
         <font>
            <Font size="14.0" />
         </font></Label>
      <TextField fx:id="userText" layoutX="271.0" layoutY="180.0" />
      <PasswordField fx:id="passwordText" layoutX="271.0" layoutY="241.0" />
      <ButtonBar layoutX="218.0" layoutY="303.0" prefHeight="40.0" prefWidth="200.0">
         <buttons>
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginAction" text="Login">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="resetButton" mnemonicParsing="false" onAction="#resetAction" text="Reset">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </buttons>
      </ButtonBar>
      <Label layoutX="132.0" layoutY="78.0" text="Welcome to Car Warehouse !!">
         <font>
            <Font name="Calisto MT Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
